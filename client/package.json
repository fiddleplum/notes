{
	"name": "@fiddleplum/notes",
	"version": "0.0.0",
	"description": "A note taking app.",
	"private": false,
	"scripts": {
		"setup": "yarn install; mkdir -p dist; cp src/config.example.js dist/config.js",
		"update": "yarn install --no-default-rc",
		"build": "rollup -c && tsc",
		"build-production": "webpack --config webpack.prod.js",
		"start": "rollup -c && tsc"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fiddleplum/notes.git"
	},
	"author": "Stephen Hurley",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fiddleplum/notes/issues"
	},
	"homepage": "https://github.com/fiddleplum/notes",
	"devDependencies": {
		"eslint": "latest",
		"@typescript-eslint/parser": "latest",
		"@typescript-eslint/eslint-plugin": "latest",
		"raw-loader": "latest",
		"rollup": "latest",
		"rollup-plugin-copy": "latest",
		"rollup-plugin-livereload": "latest",
		"rollup-plugin-serve": "latest",
		"rollup-plugin-terser": "latest",
		"rollup-plugin-typescript2": "latest",
		"ts-loader": "latest",
		"typescript": "latest",
		"webpack": "latest",
		"webpack-cli": "latest",
		"webpack-dev-server": "latest",
		"webpack-merge": "latest",
		"copy-webpack-plugin": "latest"
	}
}
